{
  "name": "universal-app",
  "version": "0.1.0",
  "description": "Universal react app",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && cross-env NODE_ENV=development node server/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production node server/index.js",
    "prod": "npm run build && cross-env NODE_ENV=production node build-server/main.js",
    "clean": "rimraf build-client build-server",
    "postinstall": "cd node_modules/polyfill-service && npm run build"
  },
  "keywords": [
    "universal",
    "react"
  ],
